{
  "name": "actions-repo-file-sync",
  "version": "1.0.0",
  "description": "This action will synchronizes files from the specified repository and creates a Pull Request with generated GitHub App token.",
  "main": "index.js",
  "devDependencies": {
    "@types/node": "^20.14.11",
    "typescript": "^5.5.3"
  },
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@octokit/rest": "^21.0.1",
    "@types/js-yaml": "^4.0.9",
    "js-yaml": "^4.1.0"
  }
}
